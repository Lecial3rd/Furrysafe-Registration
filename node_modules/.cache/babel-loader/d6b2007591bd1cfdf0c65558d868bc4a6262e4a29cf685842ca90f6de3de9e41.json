ted() {\r\n    const { data: { session } } = await supabase.auth.getSession();\r\n    if (session) {\r\n      this.user = session.user;\r\n      this.getImages();\r\n    }\r\n  },\r\n  methods: {\r\n    async magicLinkLogin() {\r\n      const { error } = await supabase.auth.signInWithOtp({ email: this.email });\r\n      if (error) {\r\n        alert('Error logging in');\r\n        console.log(error);\r\n      } else {\r\n        alert('Check your email for the magic link');\r\n      }\r\n    },\r\n    async signOut() {\r\n      const { error } = await supabase.auth.signOut();\r\n      if (error) {\r\n        alert('Error signing out');\r\n      } else {\r\n        this.user = null;\r\n        this.images = [];\r\n      }\r\n    },\r\n    async getImages() {\r\n      const { data, error } = await supabase.storage.from('images').list(this.user.id + '/', {\r\n        limit: 100,\r\n        offset: 0,\r\n        sortBy: { column: 'name', order: 'asc' },\r\n      });\r\n      if (data) {\r\n        this.images = data;\r\n      } else {\r\n        alert('Error loading images');\r\n        console.log(error);\r\n      }\r\n    },\r\n    async uploadImage(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const { data, error } = await supabase.storage\r\n        .from('images')\r\n        .upload(this.user.id + '/' + uuidv4(), file);\r\n\r\n      if (data) {\r\n        this.getImages();\r\n      } else {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async deleteImage(imageName) {\r\n      const { error } = await supabase.storage.from('images').remove([this.user.id + '/' + imageName]);\r\n      if (error) {\r\n        alert('Error deleting image');\r\n        console.log(error);\r\n      } else {\r\n        this.getImages();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.image-upload {\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n.images {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justi