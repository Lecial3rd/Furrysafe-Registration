{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      user_email: '',\n      user_password: '',\n      userDetails: {\n        firstname: '',\n        mi: '',\n        lastname: '',\n        user_username: '',\n        date_of_birth: '',\n        gender: ''\n      }\n    };\n  },\n  methods: {\n    async submitForm() {\n      const data = {\n        user_email: this.user_email,\n        user_password: this.user_password,\n        userDetails: this.userDetails\n      };\n      try {\n        const response = await axios.post('http://localhost:5000/register', data);\n        alert('User created successfully!');\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Failed to create user.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user_email","user_password","userDetails","firstname","mi","lastname","user_username","date_of_birth","gender","methods","submitForm","response","post","alert","error","console"],"sources":["C:\\Users\\Lenovo\\Downloads\\FurrySafe\\furrysafe_registration_testing-main\\frontend\\src\\components\\userRegistration.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-form\">\r\n    <form @submit.prevent=\"submitForm\">\r\n      <div>\r\n        <label for=\"firstName\">First Name</label>\r\n        <input type=\"text\" id=\"firstName\" v-model=\"userDetails.firstname\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"middleName\">Middle Name</label>\r\n        <input type=\"text\" id=\"middleName\" v-model=\"userDetails.mi\">\r\n      </div>\r\n      <div>\r\n        <label for=\"lastName\">Last Name</label>\r\n        <input type=\"text\" id=\"lastName\" v-model=\"userDetails.lastname\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"username\">Username</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"userDetails.user_username\" required>\r\n      </div>\r\n      <div>\r\n        <label for=