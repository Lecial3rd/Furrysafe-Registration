{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      user_email: '',\n      user_password: '',\n      userDetails: {\n        firstname: '',\n        mi: '',\n        lastname: '',\n        user_username: '',\n        date_of_birth: '',\n        gender: 'M'\n      },\n      errorMessage: '',\n      successMessage: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      const data = {\n        user_email: this.user_email,\n        user_password: this.user_password,\n        user_details: this.userDetails\n      };\n      try {\n        const response = await axios.post('http://localhost:5000/user', data);\n        this.successMessage = 'User created successfully!';\n        this.errorMessage = '';\n      } catch (error) {\n        console.error('Error:', error.response.data);\n        this.errorMessage = `Failed to create user: ${error.response.data}`;\n        this.successMessage = '';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user_email","user_password","userDetails","firstname","mi","lastname","user_username","date_of_birth","gender","errorMessage","successMessage","methods","submitForm","user_details","response","post","error","console"],"sources":["C:\\Users\\Lenovo\\Downloads\\FurrySafe\\furrysafe_registration_testing-main\\frontend\\src\\components\\userRegistration.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-form\">\r\n    <form @submit.prevent=\"submitForm\">\r\n      <div>\r\n        <label for=\"firstName\">First Name</label>\r\n        <input type=\"text\" id=\"firstName\" v-model=\"userDetails.firstname\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"middleName\">Middle Name</label>\r\n        <input type=\"text\" id=\"middleName\" v-model=\"userDetails.mi\">\r\n      </div>\r\n      <div>\r\n        <label for=\"lastName\">Last Name</label>\r\n        <input type=\"text\" id=\"lastName\" v-model=\"userDetails.lastname\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"username\">Username</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"userDetails.user_username\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"email\">Email</label>\r\n        <input type=\"email\" id=\"email\" v-model=\"user_email\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"password\">Password</label>\r\n        <input type=\"password\" id=\"password\" v-model=\"user_password\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"birthdate\">Birthday</label>\r\n        <input type=\"date\" id=\"birthdate\" v-model=\"userDetails.date_of_birth\" required>\r\n      </div>\r\n      <div>\r\n        <label>Gender</label>\r\n        <div class=\"gender-options\">\r\n          <input type=\"radio\" id=\"male\" value=\"M\" v-model=\"userDetails.gender\">\r\n          <label for=\"male\">Male</label>\r\n          <input type=\"radio\" id=\"female\" value=\"F\" v-model=\"userDetails.gender\">\r\n          <label for=\"female\">Female</label>\r\n          <input type=\"radio\" id=\"preferNotToSay\" value=\"\" v-model=\"userDetails.gender\">\r\n          <label for=\"preferNotToSay\">Prefer not to say</label>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\">Sign up</button>\r\n      <p v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</p>\r\n      <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user_email: '',\r\n      user_password: '',\r\n      userDetails: {\r\n        firstname: '',\r\n        mi: '',\r\n        lastname: '',\r\n        user_username: '',\r\n        date_of_birth: '',\r\n        gender: 'M'\r\n      },\r\n      errorMessage: '',\r\n      successMessage: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      const data = {\r\n        user_email: this.user_email,\r\n        user_password: this.user_password,\r\n        user_details: this.userDetails\r\n      };\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:5000/user', data);\r\n        this.successMessage = 'User created successfully!';\r\n        this.errorMessage = '';\r\n      } catch (error) {\r\n        console.error('Error:', error.response.data);\r\n        this.errorMessage = `Failed to create user: ${error.response.data}`;\r\n        this.successMessage = '';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.user-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 10px;\r\n}\r\ndiv {\r\n  margin-bottom: 15px;\r\n}\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\ninput[type=\"text\"], input[type=\"email\"], input[type=\"password\"], input[type=\"date\"], select {\r\n  width: 100%;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n}\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007BFF;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n.gender-options {\r\n  display: flex;\r\n  gap: 10px;\r\n  align-items: center;\r\n}\r\n.error {\r\n  color: red;\r\n}\r\n.success {\r\n  color: green;\r\n}\r\n</style>\r\n"],"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,aAAa,EAAE,EAAE;QACjBC,MAAM,EAAE;MACV,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMb,IAAG,GAAI;QACXC,UAAU,EAAE,IAAI,CAACA,UAAU;QAC3BC,aAAa,EAAE,IAAI,CAACA,aAAa;QACjCY,YAAY,EAAE,IAAI,CAACX;MACrB,CAAC;MAED,IAAI;QACF,MAAMY,QAAO,GAAI,MAAMhB,KAAK,CAACiB,IAAI,CAAC,4BAA4B,EAAEhB,IAAI,CA