{"ast":null,"code":"import L from 'leaflet';\nimport 'leaflet/dist/leaflet.css';\nexport default {\n  name: 'MapWithPin',\n  data() {\n    return {\n      map: null,\n      marker: null\n    };\n  },\n  mounted() {\n    this.initializeMap();\n  },\n  methods: {\n    initializeMap() {\n      // Initialize the map and set its view to the user's location\n      this.map = L.map('map').setView([10.3157, 123.8854], 13); // Example: Cebu City coordinates\n\n      // Add the tile layer (the map itself)\n      L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {\n        maxZoom: 19,\n        attribution: '© OpenStreetMap'\n      }).addTo(this.map);\n\n      // Add a click event to place a marker\n      this.map.on('click', this.addMarker);\n    },\n    addMarker(e) {\n      // If there's an existing marker, remove it\n      if (this.marker) {\n        this.map.removeLayer(this.marker);\n      }\n\n      // Add a new marker to the map at the clicked location\n      this.marker = L.marker([e.latlng.lat, e.latlng.lng]).addTo(this.map);\n\n      // Optionally, you can show the coordinates or other info\n      this.marker.bindPopup(`Coordinates: ${e.latlng.lat}, ${e.latlng.lng}`).openPopup();\n    }\n  }\n};","map":{"version":3,"names":["L","name","data","map","marker","mounted","initializeMap","methods","setView","tileLayer","maxZoom","attribution","addTo","on","addMarker","e","removeLayer","latlng","lat","lng","bindPopup","openPopup"],"sources":["C:\\Users\\Lenovo\\Downloads\\FurrySafe\\furrysafe_registration_testing-main\\frontend\\src\\components\\MapWithPin.vue"],"sourcesContent":["<template>\r\n  <div id=\"map\" :style=\"{ height: '500px', width: '100%' }\"></div>\r\n</template>\r\n\r\n<script>\r\nimport L from 'leaflet';\r\nimport 'leaflet/dist/leaflet.css';\r\n\r\nexport default {\r\n  name: 'MapWithPin',\r\n  data() {\r\n    return {\r\n      map: null,\r\n      marker: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initializeMap();\r\n  },\r\n  methods: {\r\n    initializeMap() {\r\n      // Initialize the map and set its view to the user's location\r\n      this.map = L.map('map').setView([10.3157, 123.8854], 13); // Example: Cebu City coordinates\r\n\r\n      // Add the tile layer (the map itself)\r\n      L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {\r\n        maxZoom: 19,\r\n        attribution: '© OpenStreetMap'\r\n      }).addTo(this.map);\r\n\r\n      // Add a click event to place a marker\r\n      this.map.on('click', this.addMarker);\r\n    },\r\n    addMarker(e) {\r\n      // If there's an existing marker, remove it\r\n      if (this.marker) {\r\n        this.map.removeLayer(this.marker);\r\n      }\r\n\r\n      // Add a new marker to the map at the clicked location\r\n      this.marker = L.marker([e.latlng.lat, e.latlng.lng]).addTo(this.map);\r\n\r\n      // Optionally, you can show the coordinates or other info\r\n      this.marker.bindPopup(`Coordinates: ${e.latlng.lat}, ${e.latlng.lng}`).openPopup();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.leaflet-container {\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AAKA,OAAOA,CAAA,MAAO,SAAS;AACvB,OAAO,0BAA0B;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,IAAI;MACTC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPD,aAAaA,CAAA,EAAG;MACd;MACA,IAAI,CAACH,GAAE,GAAIH,CAAC,CAACG,GAAG,CAAC,KAAK,CAAC,CAACK,OAAO,CAAC,CAAC,OAAO,EAAE,QAAQ,CAAC,EAAE,EAAE,CAAC,EAAE;;MAE1D;MACAR,CAAC,CAACS,SAAS,CAAC,oDAAoD,EAAE;QAChEC,OAAO,EAAE,EAAE;QACXC,WAAW,EAAE;MACf,CAAC,CAAC,CAACC,KAAK,CAAC,IAAI,CAACT,GAAG,CAAC;;MAElB;MACA,IAAI,CAACA,GAAG,CAACU,EAAE,CAAC,OAAO,EAAE,IAAI,CAACC,SAAS,CAAC;IACtC,CAAC;IACDA,SAASA,CAACC,CAAC,EAAE;MACX;MACA,IAAI,IAAI,CAACX,MAAM,EAAE;QACf,IAAI,CAACD,GAAG,CAACa,WAAW,CAAC,IAAI,CAACZ,MAAM,CAAC;MACnC;;MAEA;MACA,IAAI,CAACA,MAAK,GAAIJ,CAAC,CAACI,MAAM,CAAC,CAACW,CAAC,CAACE,MAAM,CAACC,GAAG,EAAEH,CA